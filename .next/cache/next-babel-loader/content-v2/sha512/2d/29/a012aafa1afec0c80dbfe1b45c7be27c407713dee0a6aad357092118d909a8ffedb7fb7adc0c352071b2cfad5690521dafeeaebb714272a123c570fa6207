ayout/Layout';\nimport store from '../store/store';\nconst isServer = typeof window === 'undefined';\n\nconst App = () => {\n  if (isServer) {\n    const { StaticRouter } = require('react-router');\n    return (\n      <StaticRouter location={this.props.router.asPath}>\n        <Layout>\n          <Switch>\n            <Route exact path=\"/pagenotfound\">\n              <PageNotFound />\n            </Route>\n            <Route path=\"/\">\n              <Provider store={store}>\n                <Header />\n                <Body />\n                <Footer />\n              </Provider>\n            </Route>\n          </Switch>\n        </Layout>\n      </StaticRouter>\n    );\n  }\n  return (\n    <BrowserRouter>\n      <Layout>\n        <Switch>\n          <Route exact path=\"/pagenotfound\">\n            <PageNotFound />\n          </Route>\n          <Route path=\"/\">\n            <Provider store={store}>\n              <Header />\n              <Body />\n              <Footer />\n            </Provider>\n          </Route>\n        </Switch>\n      </Layout>\n    </BrowserRouter>\n  );\n};\n\nexport default App;\n"]},"metadata":{},"sourceType":"module"}